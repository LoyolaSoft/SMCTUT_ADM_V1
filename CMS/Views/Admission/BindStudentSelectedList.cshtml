@model CMS.ViewModel.ViewModel.AdmissionViewModel
@{Layout = null; }
@{int count = 0;}
<div class="table-responsive">
    <table class="table table-hover table-bordered" id="AdmittedList">
        <thead>
            <tr>
                <th>#</th>                              
                <th>Name</th>
                <th>Application No</th>
                <th>Programme Name</th>
                <th>Mobile</th>
                <th>Mark Secured</th>
                <th>Main Subject Total</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @if (Model.liSelectionProcess != null)
            {
                foreach (var item in Model.liSelectionProcess)
                {
                    <tr>
                        <td>
                            @{ count++;}@count
                        </td>                      
                      
                        <td>@item.FIRST_NAME</td>
                        <td>@item.APPLICATION_NO</td>
                        <td>@item.PROGRAMME_NAME</td>
                        <td>@item.SMS_MOBILE_NO</td>
                        <td>@item.TOTAL_SECURED</td>
                        <td>@item.TOTAL_CUT_OFF_MARK</td>
                        <td>@item.STATUS_NAME</td>
                        <td><a class="btn btn-danger" href="#" onclick="SaveSelectionCancele(@item.SELECTION_ID,@item.RECEIVE_ID,@item.PROGRAMME_ID)">Cancel</a></td>
                    </tr>
                                    }
                                }
                                

                            </tbody>
                        </table>
                    </div>
