<script>
    $('.selectpicker').selectpicker({
        size: 6
    });

    var count=2;var nFile=1;
    $("input[name='consolidate_radio']").change(function()
    {
        if ($('#rbtnconsolidateYes').is(":checked"))
        {
            nFile=3;
          //   $('#dvConsolidateMarksheet').removeClass('hidden');
            $('#UploadSemesterFiles').empty();
            var clone = $("#dvImportImage1").clone();
            clone.attr("id", "dvImportImage1");
            clone.find("#Marksheet1").attr("id","Marksheet2");
            clone.find("#btnImage1").attr("id","btnImage2");
            clone.find("#img1").attr("id", "img2");
            clone.find("#img2").attr("src","");
            clone.find('#Marksheet2').attr("name","img2");
            clone.appendTo('#UploadSemesterFiles');
        }
        else
        {
            $('#UploadSemesterFiles').empty(); var nFile = 0; var nArray = ["I", "II", "III", "IV", "V","VI"];
            for (var i = 2; i <= 7; i++)
            {
                var clone = $("#dvImportImage1").clone();
                clone.attr("id", "dvImportImage1");
                clone.find("#Marksheet1").attr("id","Marksheet"+i);
                clone.find("#btnImage1").attr("id","btnImage"+i);
                clone.find("#btnImage"+i).empty();
                clone.find("#btnImage" + i).append(nArray[nFile]+" - Semester Marksheet");
                clone.find("#img1").attr("id", "img" + i);
                clone.find("#img"+i).attr("src","");
                clone.find('#Marksheet'+i).attr("name","img"+i);
                clone.appendTo('#UploadSemesterFiles');
                nFile++;
            }
        }

    });

    $("input[name='Physical_radio']").change(function()
    {

        if ($('#rbtnPhysicalYes').is(":checked"))
        {
            $('#physicaly').removeClass('hidden')
            $('#divphysicalytpe').removeClass('hidden')

            if($('#rbtnPhysicalYes').attr('status')=="0")
            {

                $('#physicalychallanged').attr("data-parsley-group","wizard-step-1");
            }
        }
        else
        {
            $('#physicaly').addClass('hidden')
            $('#divphysicalytpe').addClass('hidden')
            $('#physicalychallanged').attr("data-parsley-group","");
        }

    });
    $("input[name='Army_radio']").change(function()
    {

        if ($('#rbtnArmyYes').is(":checked"))
        {
            $('#exserviceman').removeClass('hidden')
            if($('#rbtnArmyYes').attr('status')=="0")
            {
                $('#exservice').attr("data-parsley-group","wizard-step-1");
            }
        }
        else
        {
            $('#exserviceman').addClass('hidden')
            $('#exservice').attr("data-parsley-group","");
        }

    });

    var message= @Html.Raw(Json.Encode(ViewBag.Message));

    if(message=="")
    {
        myDropzone = new Dropzone('#demo-upload', {
            addRemoveLinks: true,
            autoProcessQueue: false,
            uploadMultiple: true,
            parallelUploads: 100,
            maxFiles: 10,
            maxFilesize: 0.75,
            paramName: 'file',
            acceptedFiles: ".jpeg,.jpg,.png,.gif",
            clickable: true,
            url: '/Admission/SaveUploadedFiles',
            init: function () {
                var myDropzone = this;
                // Update selector to match your button

            },
            error: function (file, response){
                if ($.type(response) === "string")
                    var message = response; //dropzone sends it's own error messages in string
                else
                    var message = response.message;
                file.previewElement.classList.add("dz-error");
                _ref = file.previewElement.querySelectorAll("[data-dz-errormessage]");
                _results = [];
                for (_i = 0, _len = _ref.length; _i < _len; _i++) {
                    node = _ref[_i];
                    _results.push(node.textContent = message);
                }
                return _results;
            },
