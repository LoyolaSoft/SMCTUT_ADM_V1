<body class="pace-top">
    <!-- begin #page-loader -->
    <div id="page-loader" class="fade in"><span class="spinner"></span></div>
    <!-- end #page-loader -->
    <!-- begin #page-container -->
    <div id="page-container" class="fade">
        <!-- begin error -->
        <div class="error">@if (Session[Common.SESSION_VARIABLES.USER_ROLE_NAME] != null)
        {
            <div class="error-code m-b-10  ">500 <i class="ion-android-warning"></i></div>
            <div class="error-content bg-gradient-blue">
                <div class="error-message">Internal Server Error</div>
                <div class="error-desc m-b-20 text-white ">
                    Sorry for the Inconvenience...Try Again..
                </div>
                <div>
                   
                        <a href="@Url.Action("DashboardIndex","dashboard")"  class="btn btn-success">Go Back to Home Page</a>
                   
                </div>
            </div> }
        else
        {<div class="error-code m-b-10">408 <i class="ion-android-warning"></i></div>
        <div class="error-content bg-gradient-blue">
            <div class="error-message">Session Time Out</div>
            <div class="error-desc m-b-20 text-white ">
       Try Again...
            </div>
            <div>

                <a href="@Url.Action("Index","Account")" class="btn btn-success">Your Session is Expired Click to  Login Page</a>

            </div>
        </div>
          

        }
        </div>
        <!-- end error -->
       
    </div>
    <!-- end page container -->
