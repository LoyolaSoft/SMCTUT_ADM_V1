@model CMS.ViewModel.ViewModel.FeeTransactionViewModel
@{int count = 0;}
@if (Model.liCollegeDetails != null)
{
    foreach (var item in Model.liCollegeDetails)
    {
        <center>
            <table>
                <tr>
                    <td><center><strong>@item.COLLEGE_NAME</strong></center></td>
                </tr>
                <tr>
                    <td><b> @item.GRADE</b></td>
                </tr>
                <tr>
                    <td class="text-center">
                        @item.UNIVERSITY
                    </td>
                </tr>
            </table>
        </center>
    }
}
<table class="table table-hover" id="DateWiseStudentFeePaidStatusList">
    <thead>
        <tr>
            <th>#</th>
            <th>Month</th>
            <th>Date</th>
            <th>Particulars</th>
            <th>Amount</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.liFeeTransaction != null && Model.liMonth != null)
        {
            foreach (var item in Model.liMonth)
            {
                foreach (var list in Model.liFeeTransaction)
                {
                    var TempList = Model.liMonth.Where(s => s.MONTH == list.MONTH).ToList();
                    if (item.MONTH==list.MONTH)
                    {
                        <tr>
                            <td>@{ count++;}@count</td>
                            <td>@list.MONTH</td>
                            <td>@list.PAYMENT_DATE</td>
                            <td>@list.FREQUENCY_NAME</td>
                            <td>@list.COLLECTED</td>
                            <td></td>
                        </tr>
                                    }

                                }
                            <tr>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td>@item.COLLECTED</td>
                            </tr>
                                }
                            }
                            else
                            {
                                <tr>
                                    <th>No Records Availabe...!!!</th>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            }
    </tbody>
</table>
